body {
    padding: 0;
    padding-bottom: 20px;
}

@media (min-width: 768px) {
    /* Make buttons flush with left hand side iff navbar expanded */
    #navbar-collapsible, nav .container-fluid {
        padding-left: 0;
    }
}

.navbar {
    border-top: none;
    border-radius: 0 0 4px 4px;
    overflow: hidden;
}
